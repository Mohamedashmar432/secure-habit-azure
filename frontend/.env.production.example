# Production Environment Configuration for Frontend
# Copy this file to .env.production and fill in your actual values

# Backend API URL (update with your Render deployment URL)
VITE_API_URL=https://securehabit-backend.onrender.com

# Application Configuration
VITE_APP_NAME=Secure Habit
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Enterprise Security Platform

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Security Configuration
VITE_ENABLE_HTTPS_ONLY=true